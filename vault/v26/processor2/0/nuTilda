FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.14;

boundaryField
{
    front_and_back
    {
        type            empty;
    }
    tunnel
    {
        type            calculated;
        value           uniform 0;
    }
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           nonuniform 
0
;
    }
    wing
    {
        type            fixedValue;
        value           uniform 0;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform 0.14;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 0.14;
    }
}


